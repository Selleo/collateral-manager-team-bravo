<h1>Configuration</h1>
<%= link_to 'Back', root_path %>
<br>
  <thead>
    <tr>
      <th><strong>Available Tag Kinds:</strong>
 <table></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_kind.each do |tag_kind| %>
      <tr>
        <td><%= tag_kind.kind %></td>
        <td><%= link_to 'Show', tag_kind %></td>
        <td><%= link_to 'Edit', edit_tag_kind_path(tag_kind) %></td>
        <td><%= link_to 'Destroy', tag_kind, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "New Tag Kind", new_tag_kind_path %>
 
